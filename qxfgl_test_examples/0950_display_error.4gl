MAIN
   DEFINE
      v_flt FLOAT

   WHENEVER ANY ERROR CONTINUE

   LET v_flt = 9.99999999999999e-128
   DISPLAY v_flt
   LET v_flt = 9.99e+32
   DISPLAY v_flt
   LET v_flt = 9.99999999999999e+124
   DISPLAY v_flt

END MAIN
