DATABASE stores

MAIN
	DEFINE char_str CHAR(10)

	WHENEVER ANY ERROR CONTINUE
	DROP TABLE empty_char_5571
	IF SQLCA.SQLCODE != 0 THEN DISPLAY STATUS, "|", SQLCA END IF

	WHENEVER ANY ERROR STOP
	CREATE TABLE empty_char_5571 (f1 CHAR(10))
	IF SQLCA.SQLCODE != 0 THEN DISPLAY STATUS, "|", SQLCA END IF

	LOAD FROM "5571/empty_char.unl" INSERT INTO empty_char_5571
	IF SQLCA.SQLCODE != 0 THEN DISPLAY STATUS, "|", SQLCA END IF
	
	DECLARE cur CURSOR FOR SELECT f1 FROM empty_char_5571
	OPEN cur
	FOREACH cur INTO char_str
		DISPLAY char_str, "|", char_str IS NULL
	END FOREACH
	CLOSE cur
	
	CALL fgl_getkey()
END MAIN